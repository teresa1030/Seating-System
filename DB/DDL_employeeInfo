create table employeeInfo
(
    emp_id  int         not null
        primary key,
    name    varchar(32) null,
    email   varchar(32) null,
    seat_id int         not null,
    constraint employeeInfo_seatInfo_floor_seat_seq_fk
        foreign key (seat_id) references seatjdbc.seatInfo (floor_seat_seq)
);